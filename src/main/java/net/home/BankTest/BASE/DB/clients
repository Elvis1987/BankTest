CREATE TABLE clients
(
  clientId       INT AUTO_INCREMENT
    PRIMARY KEY,
  name           VARCHAR(50)                   NOT NULL,
  address        VARCHAR(50)                   NOT NULL,
  addressLive    VARCHAR(50)                   NULL,
  indetification VARCHAR(30)                   NOT NULL,
  phone          VARCHAR(15) DEFAULT 'Unknown' NULL,
  email          VARCHAR(30)                   NULL,
  cards          INT                           NULL
)